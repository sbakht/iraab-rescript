{
  "name": "rescript-react-realworld-example-app",
  "version": "1.0.0",
  "keywords": [
    "ReasonML",
    "BuckleScript",
    "reason-react",
    "ReScript",
    "rescript-react",
    "react"
  ],
  "license": "MIT",
  "author": "Jihchi Lee <achi@987.tw>",
  "scripts": {
    "build": "vite build",
    "clean": "rescript clean",
    "format": "npm run format:js && npm run format:res",
    "format:js": "prettier --write \"**/*.{js,json,md,yml}\"",
    "format:res": "rescript format -all",
    "serve": "vite preview",
    "start": "vite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-packagejson"
    ]
  },
  "dependencies": {
    "@glennsl/rescript-fetch": "^0.2.0",
    "@rescript/core": "^0.3.1",
    "@rescript/react": "^0.11.0",
    "dompurify": "^3.0.3",
    "marked": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rescript": "11.0.0-alpha.6",
    "rescript-webapi": "^0.8.0"
  },
  "devDependencies": {
    "@jihchi/vite-plugin-rescript": "^5.3.0",
    "@vitejs/plugin-react": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "vite": "^4.3.9"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": "^16",
    "pnpm": "^8"
  }
}
