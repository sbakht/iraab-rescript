{
  "name": "rescript-react-realworld-example-app",
  "namespace": true,
  "suffix": ".bs.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header",
    "-open RescriptCore"
  ],
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "bs-dependencies": [
    "@glennsl/rescript-fetch",
    "@rescript/core",
    "@rescript/react",
    "rescript-webapi"
  ],
  "bs-dev-dependencies": []
}
